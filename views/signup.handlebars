<div class="container-fluid" id="signupPage">
    <hr>
    <!--Signup Page-->
    <div class="row mt-3" id="signUpFormRow" data-aos="zoom-out-down">
        <div class="col-md-10" id="signUpFormCol">
            <div class="form-div">
                <form id="signUpForm">
                    <div class="form-group">
                        <label for="name" id="createUserNameLabel">Username</label>
                        <input type="name" class="form-control" id="createUserNameInput" rows="1" placeholder="Username">
                    </div>
                    <div class="form-group">
                        <label for="name" id="firstNameLabel">First Name</label>
                        <input type="name" class="form-control" id="firstNameInput" rows="1" placeholder="John">
                    </div>
                    <div class="form-group">
                        <label for="name" id="middleNameLabel">Middle Name</label>
                        <input type="name" class="form-control" id="middleNameInput" rows="1" placeholder="James">
                    </div>
                    <div class="form-group">
                        <label for="name" id="lastNameLabel">Last Name</label>
                        <input type="name" class="form-control" id="lastNameInput" rows="1" placeholder="Doe">
                    </div>
                    <div class="form-group">
                        <label for="photo" id="photoLabel">Photo</label>
                        <input type="photo" class="form-control" id="photoInput" rows="1" placeholder="http://via.placeholder.com/350x150">
                    </div>
                    <div class="form-group">
                        <label for="number" id="ageLabel">Age</label>
                        <input type="number" class="form-control" id="ageInput" rows="1" placeholder="">
                    </div>
                    <div class="form-group">
                        <label for="number" id="phoneLabel">Phone Number</label>
                        <input type="number" class="form-control" id="phoneInput" rows="1" placeholder="">
                    </div>
                    <div class="form-group">
                        <label for="email" id="emailLabel">Email</label>
                        <input type="email" class="form-control" id="emailInput" rows="1" placeholder="example@gmail.com">
                    </div>
                    <div class="form-group">
                        <label for="password" id="passwordLabel">Password</label>
                        <input type="password" class="form-control" id="passwordInput" rows="1" placeholder="password">
                    </div>
                    <button type="submit" id="signUpSubmitBtn" class="btn btn-primary">Submit</button>
                </form>
            </div>
        </div>
    </div>
    <!--End Of Signup Page-->
</div>